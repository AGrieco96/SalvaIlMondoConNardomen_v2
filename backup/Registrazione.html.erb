<!-- Default form register -->
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), :html =>{class:"text-center border border-light p-5"} )do |f| %>
  <%= devise_error_messages! %>

    <p class="h4 mb-4">SIGN UP</p>

    <div class="form-row mb-4">
        <div class="field col ">
            <!-- First name -->
            <%= f.label :nickname %><br />
            <%= f.text_field :nickname, autofocus: true ,class:"form-control" %>
        </div>
        <div class="field col">
          <%= f.label :name %><br />
          <%= f.text_field :name, autofocus: true ,class:"form-control" %>
        </div>

         <div class="field col">
          <%= f.label :last_name %><br />
          <%= f.text_field :last_name, autofocus: true ,class:"form-control" %>
        </div>
    </div>

    <!-- E-mail -->
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" ,class:"form-control mb-4" %>
    </div>
    
    <div class="field">
      <%= f.label :password %>
      <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "new-password", class:"form-control" %>
    </div>

    <div class="field"
      <%= f.label :password_confirmation %><br />
      <%= f.password_field :password_confirmation, autocomplete: "new-password" ,class:"form-control" %>
      <br><br>
    </div>

    <!-- Sign up button -->
    <div class="actions">
      <%= f.submit  "Sign up", class:"btn btn-info my-4 btn-block" %>
    </div>

    <!-- Social register -->
    <p>or sign up with:</p>

    <a type="button" class="light-blue-text mx-2">
        <i class="fa fa-facebook"></i>
    </a>
    <a type="button" class="light-blue-text mx-2">
        <i class="fa fa-twitter"></i>
    </a>
    <hr>

    <!-- Terms of service -->
    <p>By clicking
        <em>Sign up</em> you agree to our
        <a href="" target="_blank">terms of service</a> 
    </p>

<% end %>
